{
  "name": "solid-code",
  "displayName": "solid-code",
  "description": "learning",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "publisher": "marvdann",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarvDann/solid-code"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "solid-code.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "solid-code.licences",
        "title": "Software Licence Agreement"
      },
      {
        "command": "solid-code.changeEula",
        "title": "Toggle EULA"
      },
      {
        "command": "solid-code.changePrivacy",
        "title": "Toggle Privacy"
      }
    ],
    "walkthroughs": [
      {
        "id": "licences",
        "title": "Welcome to Solid Code",
        "description": "To use this extension you must accept our EULA and privacy policy.",
        "steps": [
          {
            "id": "accepteula",
            "title": "Accept EULA",
            "description": "Press the button below to accept our Software end user license agreement.\n[Accept EULA](command:solid-code.changeEula)",
            "media": {
              "markdown": "media/EULA.md"
            },
            "completionEvents": [
              "onSettingChanged:solid-code.eula"
            ]
          }, {
            "id": "acceptprivacy",
            "title": "Accept the Privacy Policy",
            "description": "You must accept our privacy policy before using this extension.\n[Accept Privacy Policy](command:solid-code.changePrivacy)",
            "media": {
              "markdown": "media/PrivacyPolicy.md"
            },
            "completionEvents": [
              "onSettingChanged:solid-code.privacy"
            ]
          }
        ]
      }
    ],
    "configuration": {
			"title": "Eggplant Test Studio",
			"properties": {
				"solid-code.eula": {
					"description": "EULA Accepted",
					"type": "boolean",
          "default": false,
          "scope": "machine-overridable",
          "readOnly": true
				},
        "solid-code.privacy": {
					"description": "Privacy Policy Accepted",
					"type": "boolean",
          "default": false,
          "scope": "machine-overridable",
          "readOnly": true
				}
			}
		},
    "viewsWelcome": [
      {
        "view": "workbench.explorer.emptyView",
        "contents": "You can have paragraphs of text here. You can have [links](https://code.visualstudio.com) to external sources or [internal commands](command:solid-code.helloWorld).\nUse new lines to have new paragraphs.\nPlace a link alone in a paragraph to make it a button\n[Hello](command:solid-code.helloWorld)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "concurrently \"webpack --watch\" \"vite build --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/types": "^7.21.5",
    "@rollup/plugin-url": "^8.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "^0.12.1",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vite": "^4.3.5",
    "vite-plugin-solid": "^2.5.0",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "solid-js": "^1.6.10"
  }
}